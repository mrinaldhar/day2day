            g = l * 255;
            b = l * 255;
        }
        else {
            if ( l < 0.5 ) {
                var_2 = l * ( 1 + s );
            }
            else {
                var_2 = ( l + s ) - ( s * l );
            }
            var_1 = 2 * l - var_2;

