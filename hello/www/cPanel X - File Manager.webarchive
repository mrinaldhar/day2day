CPANEL.nvdata = {
	// set nvdata silently
	set : function(key, value) {
		var api2_call = {
			cpanel_jsonapi_version : 2,
			cpanel_jsonapi_module : "NVData",
			cpanel_jsonapi_func : "set",
			names : key
		};
		api2_call[key] = value;
		
		var callback = {
			success : function(o) { },
			failure : function(o) { }
