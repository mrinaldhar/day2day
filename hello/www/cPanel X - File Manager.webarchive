        }
        else {
            error_dialog_closer = function() { this.cancel() };
        }

        var error_dialog = new Common_Dialog( DOM.generateId(), {
            width:   "400px",
            buttons: [
                { text:CPANEL.LANG.ok, handler:error_dialog_closer, isDefault:true }
